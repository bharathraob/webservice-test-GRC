<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from WebAppService WSDL service -->

  <service name="WebApp" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webApplicationPack"/>
      <parameter name="wsdlServiceElement" value="WebAppService"/>
      <parameter name="schemaQualified" value="http://webApplicationPack"/>
      <parameter name="wsdlServicePort" value="WebApp"/>
      <parameter name="className" value="webApplicationPack.WebAppSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="WebApp"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getQ" qname="operNS:getQ" xmlns:operNS="http://webApplicationPack" returnQName="retNS:getQReturn" xmlns:retNS="http://webApplicationPack" returnType="rtns:anyType" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getQ"/>

  </service>
</deployment>
